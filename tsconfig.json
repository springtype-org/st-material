{
	"compilerOptions": {
		"module": "commonjs",
		"declaration": true,
		"skipLibCheck": true,
		"target": "es5",
		"jsx": "react",
		"outDir": "dist",
		"jsxFactory": "tsx",
		"experimentalDecorators": true,
		"sourceMap": true,
		"resolveJsonModule": true
	},
	"exclude": ["node_modules","script", "templates"],
	"include": ["src"]
  }
  